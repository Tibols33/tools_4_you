<div class="container">
<h1>My new Tool</h1>


<div class="container">
  <h1>New List</h1>
  <%= simple_form_for @tool do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.input :photo, as: :file %>
    <%= f.submit "Submit Tool", class: "btn btn-primary" %>
  <% end %>
</div>
</div>
